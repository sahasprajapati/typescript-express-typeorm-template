{
  "name": "multer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "npm run build && NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev --watch src --respawn --transpile-only ./src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "code:format": "prettier --config .prettierrc 'src/**/*.{ts,js,css,html}' --write",
    "code:check": "prettier --config .prettierrc 'src/**/*.{ts,js,css,html}' --check",
    "prepare": "husky install &&  husky add .husky/pre-commit 'npm run code:format && npm run lint' && git add .husky/pre-commit",
    "typeorm": "ts-node-dev -r ./fix-module-alias.js ./node_modules/typeorm/cli.js",
    "db:migration:run": "npm run typeorm -- migration:run -d ./src/database/data-source.ts",
    "db:migration:generate": "npm run typeorm -- migration:generate -d ./src/database/data-source.ts",
    "db:migration:create": "npm run typeorm -- migration:create",
    "db:seed": "ts-node-dev -r ./fix-module-alias.js ./node_modules/typeorm-extension/dist/cli/index.js",
    "seed": "ts-node-dev -r ./fix-module-alias.js ./src/utils/seed.ts",
    "seed:generate": "ts-node-dev -r ./fix-module-alias.js ./src/utils/seed.ts",
    "db:s": "ts-node-dev -r ./fix-module-alias.js ./src/utils/seed.ts",
    "te": "ts-node-dev -r ./fix-module-alias.js ./src/test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1126.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "bullmq": "^1.81.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.0",
    "cors": "^2.8.5",
    "cron-decorators": "^0.1.5",
    "dotenv": "^16.0.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.18.0",
    "glob": "^8.0.1",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.2",
    "http": "^0.0.1-security",
    "ioredis": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "nodemailer": "^6.7.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "socket-controllers": "^0.1.2",
    "socket.io": "^4.5.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.12",
    "@types/node": "^17.0.29",
    "@types/nodemailer": "^6.4.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "swagger-ui-express": "^4.3.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
